#include <stdio.h>

int main (void)
{
    //definindo variáveis
    float v1, v2;

    //pegando inputs
    scanf("%.1f", &v1);
    scanf("%.1f", &v2);

    //print
    if (v1 > v2)
    {
        printf("O maior valor é %.1f", v1);
    }
    else 
    {
        printf("O maior valor é %.1f", v2);
    }
    return 0;
}
